#include <stdio.h>

int main()
{
int x;
float y;
char z;

    printf("enter the integer  value");
    scanf("%d",&x);
    printf("\nThe integer you have entered is %d",x);
    printf("\n enter the float value");
    scanf("\n %f",&y);
    printf("\n The float you entered is %f",y);
    printf("\n enter the character value ");
    scanf("\n%c",&z);
    printf("\n The entered values is char %c",z);
    

    return 0;
    }
    
