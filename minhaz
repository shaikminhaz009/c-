#include <stdio.h>
int main() {
    int num;
    printf("Enter an integer: ");
    scanf("%d", &num);
    if(num % 2 == 0)
        printf("%d is even.", num);
    else
        printf("%d is odd.", num);
    
    return 0;
}

01.
#include <stdio.h>
 
void main()
{
    int i, num, sum = 0,avg;
 
    printf("Enter an integer number \n");
    scanf ("%d", &num);
    for (i = 1; i <= num; i++)
    {
        sum = sum + i;
    }
    avg=sum/num;
    printf ("Sum of first %d natural numbers = %d\nand average is %d", num, sum,avg);
}
