#include <stdio.h>
int main()
{
   int radius;
   float PI_VALUE=3.14, area,circumf;

   printf("\nEnter radius of circle: ");
   
   scanf("%d",&radius);

  
   circle_area = PI_VALUE * radius * radius;
   printf("\nArea of circle is: %f",area);

 
   circumf = 2 * PI_VALUE *radius;
   printf("\nCircumference of circle is: %f",circle_circumf);

   return(0);
}

08.

#include<stdio.h>

 
int main() {
   int length, breadth, area,perimeter;
 
   printf("\nEnter the Length of Rectangle : ");
   scanf("%d", &length);
 
   printf("\nEnter the Breadth of Rectangle : ");
   scanf("%d", &breadth);
 
   area = length * breadth;
   
   perimeter=2*(length+breadth);

   printf("\nArea of Rectangle : %d", area);
   printf("\nPerimeter of Rectangle : %d", perimeter);

 
   return (0);
}

09.

#include <stdio.h>
 
int main()
{
    int a,b;
    int mul,loop;
     
    printf("Enter first number: ");
    scanf("%d",&a);
    printf("Enter second number: ");
    scanf("%d",&b);
     
    mul=0;

    for(loop=1;loop<=b;loop++){
        mul += a;
    }

    printf("Multiplication of %d and %d is: %d\n",a,b,mul);

    return 0;
}

10.

#include <stdio.h>
 
int main()
{
    long decimalnum, remainder, quotient;
    int octalNumber[100], i = 1, j;
 
    printf("Enter the decimal number: ");
    scanf("%ld", &decimalnum);
    quotient = decimalnum;
    while (quotient != 0)
    {
        octalNumber[i++] = quotient % 8;
        quotient = quotient / 8;
    }
    printf("Equivalent octal value of decimal no %d: ", decimalnum);
    for (j = i - 1; j > 0; j--)
        printf("%d", octalNumber[j]);
    return 0;
}

